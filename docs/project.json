{
  "name": "docs",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "./docs/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cd docs && npm run build"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cd docs && npm start"
      }
    },
    "deploy": {
      "executor": "nx-github-pages:deploy",
      "options": {
        "remote": "https://nexus.js.org/"
      },
      "dependsOn": ["build"]
    }
  }
}
